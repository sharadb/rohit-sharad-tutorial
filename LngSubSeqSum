#include <iostream>
#include <vector>
#include <cmath>
#include <cstdlib>
#include <string>


using namespace std;

int LISS(vector<int> input);

int main()
{
   vector<int> temp;
   
   temp.push_back(200);
   for ( int i = 0 ; i < 10; i++)
   temp.push_back(i*2);
   temp.push_back(10);
   temp.push_back(20);
   cout << LISS(temp);
   return 0;
}

int LISS( vector<int> input)
{
    vector<int> temp(input);
    int size = input.size();

    for ( int i = 0; i < size; i ++)
        cout << temp[i] << " " ;
    cout << endl;
    for( int i = 1; i < size; i++)
    {
        for ( int j = 0; j < i ; j++)
        if ( input[i] > input [j] && temp[i] < (temp[j] + input[i]))
                temp[i] = temp[j] + input[i];
    }
    int maximum = temp[0];
    for ( int i = 1; i < size; i ++)
        maximum= max(maximum,temp[i]);
    return maximum;
}

